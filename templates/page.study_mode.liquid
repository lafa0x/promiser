      const div = document.createElement('div');
      div.className = "item";

      const a = document.createElement('a');
      a.href = "?search=" + term + "&study=" + (study ? 'on' : '') + "&version=" + version.abbreviation;
      a.innerText = version.abbreviation;

      if (searchVersion === version.abbreviation) {
        a.className = 's-active';
      }

      div.appendChild(a);
      versionsContainer.appendChild(div);
    });
  }
  
  // Use only the first verse found
  const verse = versesResponse.data[0];
  
  // Set related verses
  const relatedContainer = document.getElementById('related-promises');
  relatedContainer.innerHTML = '';
  verse.related.forEach(related => {
    const item = document.createElement('div');
    item.className = "item";
    item.innerHTML = '<a href="/pages/study-mode?search=' + related.reference + '">' + related.reference + '</a>';

    relatedContainer.appendChild(item);
  });
  
  // Set the primary verse in all places it should go
  const resultTemplate = document.getElementById('search-result-template');
  
  const primaryVerseContainers = document.querySelectorAll('.primary-verse');
  for (const primaryVerseContainer of primaryVerseContainers) {
  
    const resultClone = resultTemplate.content.cloneNode(true);

    const itemContainer = resultClone.getElementById('item-container');

    // Append adjacent verses
    const items = [...verse.previous, verse, ...verse.next];
    const itemTemplate = resultClone.getElementById('item');

    items.forEach(item => {
      const itemClone = itemTemplate.content.cloneNode(true);

      const referenceElem = itemClone.querySelector('.verse-reference');
      const contentElem = itemClone.querySelector('.verse-content');
      const versionElem = itemClone.querySelector('.verse-version');

      referenceElem.textContent = item.reference;
      contentElem.textContent = item.content;
      versionElem.textContent = item.version;

      itemContainer.appendChild(itemClone);
    });
    
    itemTemplate.remove();
    
    primaryVerseContainer.innerHTML = '';
    primaryVerseContainer.appendChild(resultClone);
  }
  
  // Append cross references
  const container = document.querySelector('.cross-references-result');

  verse.crossReferences.forEach((cr, i) => {
    const resultClone = resultTemplate.content.cloneNode(true);
    if (i % 2 !== 0) {
      $(resultClone.querySelector('.search-result')).addClass('second-panel'); 
    }

    const itemContainer = resultClone.getElementById('item-container');

    // Append adjacent verses
    const items = [...cr.previous, cr, ...cr.next];
//     const items = [cr];
    const itemTemplate = resultClone.getElementById('item');

    items.forEach(item => {
      const itemClone = itemTemplate.content.cloneNode(true);

      const referenceElem = itemClone.querySelector('.verse-reference');
      const contentElem = itemClone.querySelector('.verse-content');
      const versionElem = itemClone.querySelector('.verse-version');

      referenceElem.textContent = item.reference;
      contentElem.textContent = item.content;
      versionElem.textContent = item.version;

      itemContainer.appendChild(itemClone);
    });

    itemTemplate.remove();

    container.appendChild(resultClone);
  });
  
  // Set context
//   document.getElementById('context').innerText = verse.context || 'Sorry, we do not have context for this yet.';
  
  // Set affirmations
  const affContainer = document.getElementById('affirmations');
  const affirmations = [];
  const getAffirmation = () => {
   	const index = Math.floor(Math.random() * verse.affirmations.length);
    if (affirmations.includes(verse.affirmations[index])) {
      return getAffirmation();
    }
  	return verse.affirmations[index];
  }
  if (verse.affirmations.length > affirmations.length) {
    affirmations.push(getAffirmation());
  }
  if (verse.affirmations.length > affirmations.length) {
    affirmations.push(getAffirmation());
  }
